    "163758822799304a91": {
      "type": "hedgren-faq",
      "blocks": {
        "3806103e-303b-45b9-87bd-f544e85f6c51": {
          "type": "faq",
          "settings": {
            "question": "Where do you deliver?",
            "answer": "Currently Hedgren delivers to customers who have an address in Albania, Austria, Belgium, Bosnia &amp; Herzegovina, Croatia, Czech Republic, Denmark, Finland, France, Germany, Greece, Hungary, Italy, Ireland, Luxembourg, Macedonia, Norway, Poland, Portugal, Romania, Russia, Serbia, Slovakia, Slovenia, Spain, Sweden, Switzerland, the Netherlands, Ukraine and United Kingdom."
          }
        },
        "de81b47e-4eeb-40c2-a8eb-265e05a6d704": {
          "type": "faq",
          "settings": {
            "question": "Is there a shipping charge for orders?",
            "answer": "We offer FREE SHIPPING on all orders to residential addresses in Austria, Belgium, Denmark, France mainland, Germany, Luxembourg and the Netherlands. Deliveries to residential addresses in Albania, Bosnia &amp; Herzegovina, Croatia, Czech Republic, Estonia, Finland, Greece, Hungary, Ireland, Italy, Latvia, Lithuania, Macedonia, Norway, Poland, Portugal, Romania, Russia, Serbia, Slovakia, Slovenia, Spain, Sweden, Switzerland, Ukraine, United Kingdom and also the islands are subject to an actual calculated transport cost which is shown in the shipping method section of the checkout process. \n<br><br>\nPlease note that we have no control over custom matters\/import duties. Delivery to countries outside the European Union might involve additional custom costs."
          }
        },
        "f2e423a7-fe54-4397-b8cc-dce6c3361f4f": {
          "type": "faq",
          "settings": {
            "question": "I want to return the order for a refund, what should I do?",
            "answer": "Returns will only be accepted within 14 days of the delivery date. <br><br>For further questions view our returns page or get in contact with customer service. Please note that we have no control over custom matters\/import duties. Delivery to countries outside the European Union might involve additional custom costs."
          }
        }
      },
