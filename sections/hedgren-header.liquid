      <ul class="">
        {%- for childlink in link.links -%}
        <li>
          {%- if childlink.links == blank -%}
          <a href="{{ childlink.url }}" class="{% if childlink.current %}active{% endif %}"{% if childlink.current %} aria-current="page"{% endif %}>
            {{ childlink.title | escape }}
          </a>
          {%- else -%}
          {{ childlink.title | escape }}

          <ul>
            {%- for grandchildlink in childlink.links -%}
            <li>
              <a href="{{ grandchildlink.url }}" class="{% if grandchildlink.current %}active{% endif %}"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                {{ grandchildlink.title | escape }}
              </a>
            </li>
            {%- endfor -%}
          </ul>
          {%- endif -%}
        </li>
        {%- endfor -%}
      </ul>
      {%- endif -%}
    </li>
    {%- endfor -%}
    {%- endif -%}
    {% endcomment %}
